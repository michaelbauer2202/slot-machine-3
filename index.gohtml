<!doctype html>
<html>
<head>
<meta charset="utf-8">
<title>Slot-Machine-3</title>
<link rel="stylesheet" href="static/slot.css">
</head>
<body>
    <div id="stage" class="perspective-on" style="padding-top:100px">
        {{range .}}
            <div class="ring">
                <img width=100 height=100 src="static/icon_{{.}}.jpg">
            </div>
        {{end}}
    </div>
    <div>
        <button onclick="location.reload()" class="go">Spin Again!</button>
    </div>
</body>
</html>